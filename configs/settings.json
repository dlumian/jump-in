{
  "steps": [
    {
      "description": "Activate the Conda environment",
      "command": ["conda", "activate", "myenv"],
      "ignore_errors": false
    },
    {
      "description": "Launch Jupyter Lab",
      "command": ["jupyter", "lab"],
      "timeout": 60,
      "ignore_errors": false
    },
    {
      "description": "Open Visual Studio Code",
      "command": ["code", "."],
      "ignore_errors": true
    }
  ]
}